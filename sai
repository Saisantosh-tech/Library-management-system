[32m+[m
[32m+[m[32mpublic class Library {[m
[32m+[m
[32m+[m[32m    private List<Book> books = new ArrayList<>();[m
[32m+[m[32m    private List<Member> members = new ArrayList<>();[m
[32m+[m
[32m+[m[32m    public void addBook(Book book) {[m
[32m+[m[32m        books.add(book);[m
[32m+[m[32m        System.out.println("Book added.");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void addMember(Member member) {[m
[32m+[m[32m        members.add(member);[m
[32m+[m[32m        System.out.println("Member added.");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void showBooks() {[m
[32m+[m[32m        books.forEach(System.out::println);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void showMembers() {[m
[32m+[m[32m        members.forEach(System.out::println);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void issueBook(int bookId) {[m
[32m+[m[32m        for (Book book : books) {[m
[32m+[m[32m            if (book.getId() == bookId && !book.isIssued()) {[m
[32m+[m[32m                book.setIssued(true);[m
[32m+[m[32m                System.out.println("Book issued.");[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        System.out.println("Book not available.");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void returnBook(int bookId) {[m
[32m+[m[32m        for (Book book : books) {[m
[32m+[m[32m            if (book.getId() == bookId && book.isIssued()) {[m
[32m+[m[32m                book.setIssued(false);[m
[32m+[m[32m                System.out.println("Book returned.");[m
[32m+[m[32m                return;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        System.out.println("Invalid return.");[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
