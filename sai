[32m+[m[32m    private boolean isIssued;[m
[32m+[m
[32m+[m[32m    public Book(int id, String title, String author) {[m
[32m+[m[32m        this.id = id;[m
[32m+[m[32m        this.title = title;[m
[32m+[m[32m        this.author = author;[m
[32m+[m[32m        this.isIssued = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Getters and Setters[m
[32m+[m[32m    public int getId() { return id; }[m
[32m+[m[32m    public String getTitle() { return title; }[m
[32m+[m[32m    public String getAuthor() { return author; }[m
[32m+[m[32m    public boolean isIssued() { return isIssued; }[m
[32m+[m[32m    public void setIssued(boolean issued) { isIssued = issued; }[m
[32m+[m
[32m+[m[32m    public String toString() {[m
[32m+[m[32m        return id + " | " + title + " | " + author + " | " + (isIssued ? "Issued" : "Available");[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
